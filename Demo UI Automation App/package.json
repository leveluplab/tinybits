{
  "version": "1.0.0",
  "description": "React Init for all react projects",
  "name": "react-init",
  "scripts": {
    "start:dev": "NODE_ENV=development npm run build:prod && NODE_ENV=development node server/index.js",
    "build:prod": "webpack --mode=production --node-env=production",
    "build:prod:server": "rm -rf server-dist/server.js && webpack --config webpack.server.config.js --mode=production",
    "start": "NODE_ENV=production node server-dist/server.js"
  },
