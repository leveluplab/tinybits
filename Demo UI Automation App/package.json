    "express": "^4.17.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "openurl": "^1.1.1",
    "postcss": "^8.4.8",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
